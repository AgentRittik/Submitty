{{ add_twig_module_js('grading/electronic/PeerPanel.js') }}
<div id="peer_info" class="rubric_panel {{ is_ta_grading ? "" : "empty" }}" data-has-active-version="{{ has_active_version }}" data-gradeable-id="{{ gradeable_id }}" data-anon-id="{{ anon_id }}">
    <div>
        {% if is_ta_grading %}
            <div id="rubric-container" class="container">
                <span class="col grading_label">Peer Grading
                    <a class="key_to_click" id="peer-edit-marks-btn" tabindex="0" title="Edit Peer Marks" aria-label="edit peer marks">
                        <i class="fas fa-pencil-alt black-btn" aria-hidden="true" style="font-size: 16px; margin: 5px;">
                        </i>
                    </a>
                </span>
                <div class="inner-container" id="peer-grading-box">
                    {% if not has_active_version %}
                        <h4>No Submission</h4>
                    {% endif %}
                </div>
            </div>
        {% else %}
            <div id="no-manual-grading-container">
                <h3> No Manual Grading For This Assignment </h3>
            </div>
        {% endif %}
    </div>
</div>
