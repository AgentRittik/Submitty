{% import _self as self %}
{ add_twig_module_js('grading/electronic/PeerResultsPanel.js') }}
<div id="peer_results" class="rubric_panel">
    <div>
        <span class="grading_label">Peer Grading Results</span>
        <span>Student Grader: </span>
        <div class="inner-container_peer_results" id="grading-box-peer" data-has-active-version="{{ has_active_version }}" data-gradeable-id="{{ gradeable_id }}" data-anon-id="{{ anon_id }}">
                    {% if not has_active_version %}
                        <h4>No Submission</h4>
                    {% endif %}
        </div>
    </div>
</div>
